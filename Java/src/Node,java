package folder_linked_list;

public class Node{
    private int value;
    private Node next;

    public Node(int val){
        this.value=val;
    }
    
    // Getter cho value
    public int getValue() {
        return value;
    }

    // Getter v√† setter cho next
    public Node getNext() {
        return next;
    }

    public void setNext(Node next) {
        this.next = next;
    }


}